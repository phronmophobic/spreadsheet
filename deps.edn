{:paths ["src" "resources"]
 :deps {org.clojure/clojure {:mvn/version "1.11.1"}
        com.phronemophobic/membrane {:mvn/version "0.10.3-beta"}
        com.phronemophobic.membrane/skialib-macosx-aarch64 {:mvn/version "0.9.31.0-beta"}
        com.phronemophobic.membrane/skialib-macosx-x86-64 {:mvn/version "0.9.31.0-beta"}
        com.phronemophobic.membrane/skialib-linux-x86-64 {:mvn/version "0.9.31.0-beta"}

        ;; com.phronemophobic/dev-db {:local/root "../dev-db"}
        com.xtdb/xtdb-core {:mvn/version "1.21.0"}
        com.xtdb/xtdb-rocksdb {:mvn/version "1.21.0"}

        com.phronemophobic/viscous {:mvn/version "1.3.2"}

        com.phronemophobic/schematic {:mvn/version "0.1.0"}
        org.clojure/core.async {:mvn/version "1.5.648"}
        mogenslund/liquid {:mvn/version "2.0.3"}
        org.clojure/tools.analyzer.jvm {:mvn/version "1.1.0"}

        ;; for spec generation
        org.clojure/test.check {:mvn/version "0.9.0"}

        ;; clojure-zulip/clojure-zulip {:local/root "../clojure-zulip"}
        org.clojure/data.xml {:mvn/version "0.0.8"}
        clj-tagsoup/clj-tagsoup {:mvn/version "0.3.0"}
        ubergraph/ubergraph {:mvn/version "0.8.2"}
        ,}
 :aliases
 {:run-m {:main-opts ["-m" "com.phronemophobic.membrane.spreadsheet"]}
  :run-x {:ns-default com.phronemophobic.membrane.spreadsheet
          :exec-fn greet
          :exec-args {:name "Clojure"}}

  :project {:extra-deps {com.clojure-goes-fast/clj-async-profiler {:mvn/version "0.5.1"}
                         com.phronemophobic/clj-reddit {:local/root "../clj-reddit"}
                         com.phronemophobic/membrane {:local/root "../membrane2"}
                         com.phronemophobic.membrane/viscous {:local/root "../viscous"}
                         com.phronemophobic/schematic {:local/root "../schematic"}}
            :jvm-opts ["-XX:+UnlockDiagnosticVMOptions"
                       "-Djdk.attach.allowAttachSelf=true"
                       "-XX:-OmitStackTraceInFastThrow"
                       "-XX:+DebugNonSafepoints"
                       "-Xmx8096M"]}

  :test {:extra-paths ["test"]
         :extra-deps {org.clojure/test.check {:mvn/version "1.1.1"}}}
  :skija {:extra-deps {org.jetbrains.skija/skija-macos-x64 {:mvn/version "0.89.18"}
                       org.lwjgl/lwjgl {:mvn/version "3.2.3"}
                       org.lwjgl/lwjgl$natives-macos {:mvn/version "3.2.3"}
                       org.lwjgl/lwjgl-glfw {:mvn/version "3.2.3"}
                       org.lwjgl/lwjgl-glfw$natives-macos {:mvn/version "3.2.3"}
                       org.lwjgl/lwjgl-opengl {:mvn/version "3.2.3"}
                       org.lwjgl/lwjgl-opengl$natives-macos {:mvn/version "3.2.3"}}}
  :runner
  {:extra-deps {com.cognitect/test-runner
                {:git/url "https://github.com/cognitect-labs/test-runner"
                 :sha "b6b3193fcc42659d7e46ecd1884a228993441182"}}
   :main-opts ["-m" "cognitect.test-runner"
               "-d" "test"]}
  :uberjar {:replace-deps {com.github.seancorfield/depstar {:mvn/version "2.1.303"}}
            :exec-fn hf.depstar/uberjar
            :exec-args {:aot true
                        :jar "spreadsheet.jar"
                        :main-class "com.phronemophobic.membrane.spreadsheet"
                        :sync-pom true}}}

 :mvn/repos
 {"space-maven" {:url "https://packages.jetbrains.team/maven/p/skija/maven"}
  "sonatype-oss-public" {:url "https://oss.sonatype.org/content/groups/public/"}}}
